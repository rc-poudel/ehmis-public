{
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react",
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strictNullChecks": true,
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "src",
    "types",
    "tailwind.config.ts",
    "cypress",
    "cypress.config.ts"
  ]
}
